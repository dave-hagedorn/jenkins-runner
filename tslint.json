{
	"rules": {
		"ordered-imports": true,
		"no-duplicate-imports": true,
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
		"quotemark": [ true, "double", "backtick" ],
		"triple-equals": true
	},
	"defaultSeverity": "warning"
}
